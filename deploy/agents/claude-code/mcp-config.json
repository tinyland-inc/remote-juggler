{
  "mcpServers": {
    "remote-juggler": {
      "command": "remote-juggler",
      "args": [
        "--mode=mcp"
      ],
      "env": {}
    },
    "rj-gateway": {
      "command": "rj-gateway",
      "args": [
        "--listen=local",
        "--chapel-bin=remote-juggler"
      ],
      "env": {
        "RJ_GATEWAY_LISTEN": "local",
        "RJ_GATEWAY_SETEC_URL": "https://setec.tail1234.ts.net"
      }
    }
  }
}
